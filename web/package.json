{
    "name": "ever-gosh",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "wasm": "cp ./node_modules/@eversdk/lib-web/eversdk.wasm ./public",
        "start": "react-scripts start",
        "start:ci": "dotenv -e .env.ci react-scripts start",
        "start:dev": "dotenv -e .env.development react-scripts start",
        "start:prod": "dotenv -e .env.production react-scripts start",
        "build:dev": "dotenv -e .env.development react-scripts build",
        "build:prod": "dotenv -e .env.production react-scripts build",
        "build:dockerdev": "rim -m 'src/**/* | package.json | tsconfig.json | postcss.config.js | tailwind.config.json | .env.docker.development | ../react-gosh/src/**/*' -s 'rim.cache.json' dotenv -e .env.docker.development react-scripts build",
        "build:dockerprod": "rim -m 'src/**/* | package.json | tsconfig.json | postcss.config.js | tailwind.config.json | .env.docker.production | ../react-gosh/src/**/*' -s 'rim.cache.json' dotenv -e .env.docker.production react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "update-react-gosh": "cd ../react-gosh && rm -rf dist && npm run build && cd ../web && rm -rf ./node_modules/react-gosh || true && rm -rf ./node_modules/.cache || true && npm update react-gosh"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "@docker/extension-api-client": "^0.2.3",
        "@emotion/react": "^11.9.3",
        "@emotion/styled": "^11.9.3",
        "@eversdk/appkit": "^0.3.1",
        "@eversdk/core": "^1.35.0",
        "@eversdk/lib-web": "^1.35.0",
        "@fortawesome/fontawesome-svg-core": "^6.1.1",
        "@fortawesome/free-brands-svg-icons": "^6.1.1",
        "@fortawesome/free-regular-svg-icons": "^6.1.1",
        "@fortawesome/free-solid-svg-icons": "^6.1.1",
        "@fortawesome/react-fontawesome": "^0.1.18",
        "@headlessui/react": "^1.6.0",
        "@isomorphic-git/lightning-fs": "^4.6.0",
        "@monaco-editor/react": "^4.3.1",
        "@mui/icons-material": "^5.8.4",
        "@mui/material": "^5.9.1",
        "buffer": "^6.0.3",
        "classnames": "^2.3.1",
        "crypto-js": "^4.1.1",
        "diff": "^5.0.0",
        "file-saver": "^2.0.5",
        "formik": "^2.2.9",
        "github-markdown-css": "^5.1.0",
        "react": "^18.2.0",
        "react-copy-to-clipboard": "^5.0.4",
        "react-dom": "^18.2.0",
        "react-gosh": "file:../react-gosh/react-gosh-1.0.0.tgz",
        "react-markdown": "^8.0.0",
        "react-query": "^3.34.19",
        "react-router-dom": "^6.2.1",
        "react-toastify": "^8.2.0",
        "recoil": "^0.7.0",
        "recoil-persist": "^4.1.0",
        "rehype-raw": "^6.1.1",
        "web-vitals": "^2.1.0",
        "yup": "^0.32.11"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^13.1.1",
        "@testing-library/user-event": "^14.1.1",
        "@types/crypto-js": "^4.1.1",
        "@types/diff": "^5.0.2",
        "@types/file-saver": "^2.0.5",
        "@types/jest": "^27.0.1",
        "@types/node": "^17.0.30",
        "@types/react": "^18.0.8",
        "@types/react-copy-to-clipboard": "^5.0.2",
        "@types/react-dom": "^18.0.3",
        "autoprefixer": "^10.4.4",
        "dotenv-cli": "^6.0.0",
        "postcss": "^8.4.12",
        "prettier": "2.7.1",
        "react-scripts": "5.0.1",
        "sass": "^1.49.7",
        "serve": "^13.0.2",
        "tailwindcss": "^3.0.23",
        "typescript": "^4.4.2"
    }
}
